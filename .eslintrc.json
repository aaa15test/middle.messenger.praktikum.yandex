{
  "extends": [
    "airbnb",
    "plugin:@typescript-eslint/recommended",
  ],
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint"],
  "rules": {
    "import/prefer-default-export": "off",
    "comma-dangle": "off",
    "@typescript-eslint/comma-dangle": "off",
    "arrow-body-style": "off"
  },
  "env": {
    "browser": true,
    "node": true
  },
  "parserOptions": {
    "project": "./tsconfig.json"
  }
}
